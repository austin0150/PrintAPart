<!DOCTYPE html>
<html lang="en">
<link rel="shortcut icon" type="image/jpg" href="images/faviconImageClear.png">
<head>
    <%- include('../partials/head'); %>
</head>
<body>

<header>
    <%- include('../partials/header'); %>

</header>

<main>
    <h1>Become Printer Forms</h1>
    <div class="container" style="margin-top: 15px;">
        <div class="card-deck" style="justify-content: center;">
            <% becomePrinterForms.forEach(function(form) { %>
                <%- include('../partials/become_printer_form_tile', {form : form.data(), formID : form.id}); %>
            <% }); %>
        </div>
    </div>


</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>

<script>
    function openBecomePrinterForm(formID)
    {
        console.log("button clicked")
        window.location.href = '/become_printer_form?formID='+formID;
    }
</script>
</html>
